<%= panel('My Recent Contributions', body_options: { class: 'front-page-panel' }) do %>
    <ul class="feed">
      <% activity = current_user.person.recent_activity(10).select {|a| a.activity_loggable} %>
      <% items = convert_logs_to_hash(activity) %>
      <% items.each do |item| %>
          <%= display_single_log_item_hash(item, 'added') -%>
      <% end %>
    </ul>
<% end %>
